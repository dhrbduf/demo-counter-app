pipeline{

    agent any

    parameters{
        choice(name: 'action', choices: 'create\ndestory\destoryekscluster', description: 'Create/Update or destory the eks cluster')
    }


    stages{
        stage('git checkout'){
            steps{
                git branch: 'main', url: 'https://github.com/dhrbduf/demo-counter-app.git'    
            }

        }

    }

}
